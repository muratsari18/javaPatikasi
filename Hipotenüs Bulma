package giris;
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
       int a,b;
       double c;

       Scanner inp = new Scanner(System.in);
     System.out.print("A kenarını giriniz:");
     a=inp.nextInt();

     System.out.print("B kenarını giriniz:");
     b=inp.nextInt();

     c=Math.sqrt((a*a) + (b*b));


     System.out.println("C kenarının " +c);

    }
}
